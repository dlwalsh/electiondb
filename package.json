{
  "name": "graphql-intro",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "AWS_PROFILE=electiondb babel-node server.js",
    "wipe": "rm -rf index.zip build/",
    "deploy:compile": "babel index.js query.js schema/* --out-dir build/",
    "deploy:lib": "cd build && yarn add graphql",
    "deploy:zip": "rm -f index.zip && cd build && zip -r -X ../index.zip *",
    "deploy:transfer": "AWS_PROFILE=adminuser aws lambda update-function-code --function-name electiondb --zip-file fileb://index.zip",
    "deploy": "yarn deploy:compile && yarn deploy:lib && yarn deploy:zip && yarn deploy:transfer"
  },
  "repository": {},
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.9.0",
    "graphql": "^0.9.1",
    "mongoose": "^4.8.5"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "csv": "^1.1.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "lodash": "^4.17.4",
    "string-format": "^0.5.0"
  }
}
